{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cf-ai-bookbuddy",
  "main": "cf-ai-bookbuddy/src/index.js",
  "compatibility_date": "2025-10-31",
  "ai": { "binding": "AI" },
  "durable_objects": {
    "bindings": [
      { "class_name": "MyDurableObject", "name": "MY_DURABLE_OBJECT" }
    ]
  },
  "migrations": [{ "new_sqlite_classes": ["MyDurableObject"], "tag": "v1" }],
  "observability": { "enabled": true }
}
